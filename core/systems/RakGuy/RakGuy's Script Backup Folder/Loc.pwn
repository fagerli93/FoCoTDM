new LocPLID[MAX_PLAYERS];

CMD:loc(playerid, params[])
{
	new LocPID;
	new LocMSG[100];
	if(sscanf(params, "u", LocPID))
	{
	    SendClientMessage(playerid, -1, "[Usage:] /loc [PLayerID/Part_Of_Name]");
	}
	else
	{
	    if(IsPlayerConnected(LocPID)==1)
	    {
	        new LocLKI = LocPLID[LocPID];
	        if(LocLKI!=-1)
	        {
	        	format(LocMSG, sizeof(LocMSG), "Player was last seen at: %s", LocCordName[LocLKI][LocName]);
	        	SendClientMessage(playerid, -1, LocMSG);
			}
			LocPLID[LocPID]=SearchNearestLoc(LocPID);
			format(LocMSG, sizeof(LocMSG), "Player is at/around: %s", LocCordName[LocPLID[LocPID]][LocName]);
            SendClientMessage(playerid, -1, LocMSG);
		}
	}
	return 1;
}

stock SearchNearestLoc(playerid)
{
	new Closest=0;
	for(new i=0;i<386;i++)
	{
	    if(GetPlayerDistanceFromPoint(playerid, LocCordName[i][LocCX],LocCordName[i][LocCY],LocCordName[i][LocCZ])<GetPlayerDistanceFromPoint(playerid,LocCordName[Closest][LocCX],LocCordName[Closest][LocCY],LocCordName[Closest][LocCZ]))
	    {
	        Closest=i;
	    }
	}
	return Closest;
}

public OnPlayerConnect(playerid)
{
	LocPLID[playerid]=-1;
	return 1;
}




