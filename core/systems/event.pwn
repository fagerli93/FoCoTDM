	else if(t2 == 0)
	{
		EndEvent();
		SendClientMessageToAll(COLOR_NOTICE, "[EVENT NEWS]: Zombies have bitten all Civilians!");
		increment = 0;
		return 1;
	}
	if(EventPlayersCount() == 1)
	{
		EndEvent();
	}
	SetPlayerWeather(playerid, 0);
	return 1;
}

